{
  "version": 1,
  "framework": "react-router",
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "routes": [
    {
      "src": "/(images|icons|assets|static)/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }

  ]
}